		machine = "NANOPI-R2";
		hwrev = <0x3>;
		model = "NanoPi NEO3S";
		phandle = <0x12a>;
	};

	fiq-debugger {
		compatible = "rockchip,fiq-debugger";
		rockchip,serial-id = <0x2>;
		rockchip,signal-irq = <0x9f>;
		rockchip,wake-irq = <0x0>;
		rockchip,irq-mode-enable = <0x0>;
		rockchip,baudrate = <0x16e360>;
		interrupts = <0x0 0x7f 0x8>;
		status = "okay";
	};

	adc-keys {
		compatible = "adc-keys";
		io-channels = <0x94 0x0>;
		io-channel-names = "buttons";
		poll-interval = <0x64>;
		keyup-threshold-microvolt = <0x1b7740>;

		vol-up-key {
			linux,code = <0x73>;
			label = "volume up";
			press-threshold-microvolt = <0x4650>;
		};
	};

	gpio-keys {
		compatible = "gpio-keys";
		pinctrl-names = "default";
		pinctrl-0 = <0x95>;
		phandle = <0x12b>;

		button@0 {
			gpios = <0x96 0x0 0x1>;
			label = "K1";
			linux,code = <0x101>;
			debounce-interval = <0x14>;
			wakeup-source;
		};
	};

	external-gmac-clock {
		compatible = "fixed-clock";
		clock-frequency = <0x7735940>;
		clock-output-names = "gmac_clkin";
		#clock-cells = <0x0>;
		phandle = <0x78>;
	};

	reserved-memory {
		#address-cells = <0x2>;
		#size-cells = <0x2>;
		ranges;

		drm-logo@00000000 {
			compatible = "rockchip,drm-logo";
			reg = <0x0 0x0 0x0 0x0>;
			phandle = <0x68>;
		};
	};

	gpio-leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <0x97>;
		status = "okay";
		phandle = <0x12c>;

		led@1 {
			gpios = <0x96 0x2 0x0>;
			label = "status_led";
			linux,default-trigger = "heartbeat";
			linux,default-trigger-delay-ms = <0x0>;
		};
	};

	sdio-pwrseq {
		compatible = "mmc-pwrseq-simple";
		clocks = <0x98 0x1>;
		clock-names = "ext_clock";
		pinctrl-names = "default";
		pinctrl-0 = <0x99>;
		status = "okay";
		reset-gpios = <0x9a 0x8 0x1>;
		phandle = <0x7f>;
	};

	sound {
		status = "disabled";
		compatible = "simple-audio-card";
		simple-audio-card,format = "i2s";
		simple-audio-card,mclk-fs = <0x100>;
		simple-audio-card,name = "rockchip,rk3328";

		simple-audio-card,cpu {
			sound-dai = <0x9b>;
		};

		simple-audio-card,codec {
			sound-dai = <0x9c>;
		};
	};

	hdmi-sound {
		compatible = "simple-audio-card";
		simple-audio-card,format = "i2s";
		simple-audio-card,mclk-fs = <0x80>;
		simple-audio-card,name = "rockchip,hdmi";

		simple-audio-card,cpu {
			sound-dai = <0x9d>;
		};

		simple-audio-card,codec {
			sound-dai = <0x9e>;
		};
	};

	spdif-sound {
		status = "disabled";
		compatible = "simple-audio-card";
		simple-audio-card,name = "rockchip,spdif";

		simple-audio-card,cpu {
			sound-dai = <0x9f>;
		};

		simple-audio-card,codec {
			sound-dai = <0xa0>;
		};
	};

	spdif-out {
		status = "disabled";
		compatible = "linux,spdif-dit";
		#sound-dai-cells = <0x0>;
		phandle = <0xa0>;
	};

	host-vbus-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc_host_vbus";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <0x4c4b40>;
		regulator-max-microvolt = <0x4c4b40>;
		phandle = <0x6d>;
	};

	otg-vbus-regulator {
		compatible = "regulator-fixed";
		gpio = <0x9a 0x1 0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <0xa1>;
		regulator-name = "vcc_otg_vbus";
		regulator-min-microvolt = <0x4c4b40>;
		regulator-max-microvolt = <0x4c4b40>;
		enable-active-high;
		phandle = <0x6b>;
	};

	vcc-phy-regulator {
		compatible = "regulator-fixed";
		regulator-name = "vcc_phy";
		regulator-always-on;
		regulator-boot-on;
		phandle = <0x77>;
	};

	sdmmc-regulator {
		compatible = "regulator-fixed";
		gpio = <0x96 0x1e 0x1>;
		pinctrl-names = "default";
		pinctrl-0 = <0xa2>;
		regulator-name = "vcc_sd";
		regulator-boot-on;
		regulator-min-microvolt = <0x325aa0>;
		regulator-max-microvolt = <0x325aa0>;
		vin-supply = <0x2d>;
		phandle = <0x72>;
	};

	sdmmcio-regulator {
		compatible = "regulator-gpio";
		gpios = <0x3d 0x1c 0x0>;
		states = <0x1b7740 0x1 0x325aa0 0x0>;
		regulator-name = "vccio_sd";
		regulator-type = "voltage";
		regulator-min-microvolt = <0x1b7740>;
		regulator-max-microvolt = <0x325aa0>;
		regulator-always-on;
		vin-supply = <0x2d>;
		startup-delay-us = <0x7d0>;
		regulator-settling-time-us = <0x1388>;
		enable-active-high;
		status = "okay";
		phandle = <0x2f>;
	};

	vcc-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc_sys";
		regulator-min-microvolt = <0x4c4b40>;
		regulator-max-microvolt = <0x4c4b40>;
		regulator-always-on;
		phandle = <0x3f>;
	};

	xin32k {
		compatible = "fixed-clock";
		clock-frequency = <0x8000>;
		clock-output-names = "xin32k";
		#clock-cells = <0x0>;
		phandle = <0x12d>;
	};

	wireless-wlan {
		compatible = "wlan-platdata";
		rockchip,grf = <0x20>;
		wifi_chip_type = "ap6255";
		sdio_vref = <0x708>;
		WIFI,host_wake_irq = <0x9a 0x1 0x0>;
		status = "okay";
		phandle = <0x12e>;
	};

	wireless-bluetooth {
		compatible = "bluetooth-platdata";
		clocks = <0x98 0x1>;
		clock-names = "ext_clock";
		status = "okay";
		phandle = <0x12f>;
	};

